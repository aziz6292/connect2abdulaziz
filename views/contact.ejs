<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>contact Abdulaziz</title>
  <link rel="icon" href="/images/favicon.png" type="image/png" />

  <link rel="stylesheet" href="/css/style.css" media="all" />
  <script src="/js/script.js"></script>

  
  <!-- Include Font Awesome CSS -->
  <script src="https://kit.fontawesome.com/e27baf0e00.js" crossorigin="anonymous"></script>
</head>
<body class="clearfix">
  <header>
    <nav>
      <div class="menu-icon">
        <i class="fas fa-bars"></i>
      </div>
      <ul class="nav-links">
        <div class="logo">
          <a href="/" class="logo-link">
            <img
              src="/images/logo.png"
              height="80px"
              width="80px"
              alt="logo"
              class="inner-logo-pic"
            />
          </a>
        </div>
        <li class="about"><a href="/about">about</a></li>
        <li class="portfolio"><a href="/portfolio">portfolio</a></li>
        <li class="skill"><a href="/skill">skill</a></li>
        <li class="project"><a href="/project">project</a></li>
        <li class="contact"><a href="/contact">contact</a></li>
        <li>
          <ul class="social">
            <li>
              <a
                href="#"
                title="Follow me on GitHub"
                target="_blank"
                class="github-icon"
                ><i class="fab fa-github"></i
              ></a>
            </li>
            <li>
              <a
                href="#"
                title="Connect with me on Linkedin"
                target="_blank"
                class="linkedin-icon"
                ><i class="fab fa-linkedin"></i
              ></a>
            </li>
            <li>
              <a
                href="#"
                title="Follow me on Twitter"
                target="_blank"
                class="twitter-icon"
                ><i class="fab fa-twitter"></i
              ></a>
            </li>
            <li>
              <a
                href="#"
                title="Follow me on Instagram"
                target="_blank"
                class="instagram-icon"
                ><i class="fab fa-instagram"></i
              ></a>
            </li>
          </ul>
        </li>
      </ul>
    </nav>
  </header>

  <div class="content about clearfix">
    <section class="main">
      <div id="text-main" class="contact-text">
        <h1>contact.</h1>
        <p class="intro">Get in touch with me via social media or email.</p>

        <ul class="social-disc">
          <li><a href="#" target="_blank" class="twitter">Twitter</a></li>
          <li><a href="#" target="_blank" class="facebook">Facebook</a></li>
          <li><a href="#" target="_blank" class="linkedin">LinkedIn</a></li>
          <li><a href="#" target="_blank" class="instagram">Instagram</a></li>
        </ul>

      </div>
      <div class="img-div">
        <img
          class="img-main img-contact"
          src="/images/profile-transformed.png"
          alt="Abdul Aziz"
        />
      </div>
    </section>
    <section id="content-detail" class="dark form">
      <div class="row">
        <div class="col-12">
          <h2>Send me an email</h2>
        </div>
        <div class="wpcf7 no-js" id="wpcf7-f16-o1" lang="en-US" dir="ltr">
          <form id="contact-form" action="/submit_form" method="post" class="wpcf7-form" novalidate="novalidate">
            <div class="flex-container">
              <!-- Left side: Name and Email -->
              <div class="left-column">
                <p>Name <input id="your-name" type="text" name="your-name" placeholder="Name" required></p>
                <p>Email <input id="your-email" type="email" name="your-email" placeholder="Email" required></p>
              </div>
              <!-- Right side: Message -->
              <div class="right-column">
                <p>Message <textarea id="your-message" rows="10" name="your-message" placeholder="Message" required></textarea></p>
                <!-- Send Button -->
                <p class="clearfix">
                  <input class="wpcf7-form-control wpcf7-submit submit" type="submit" value="Send email" />
                </p>
              </div>
            </div>
          </form>
        </div>
        <!-- Response output element -->
        <div id="response-output"></div>
      </div>
    </section>

  </div>

  <footer id="footer" role="contentinfo">
    <div class="row">
      <div class="col-12">
        <div class="left">
          <a class="transition" href="#">&copy; 2023 Abdul Aziz</a>
        </div>
        <nav id="nav-footer">
          <ul>
            <li class="about"><a href="/about">about</a></li>
            <li class="portfolio"><a href="/portfolio">portfolio</a></li>
            <li class="skill"><a href="/skill">skill</a></li>
            <li class="project"><a href="/project">project</a></li>
            <li class="contact"><a href="/contact">contact</a></li>
          </ul>
        </nav>
      </div>
    </div>
    <div class="gradient-white">
      <a class="top" href="#">Back to top</a>
    </div>
  </footer>
</body>
</html>
